{
  "name": "sockette",
  "version": "0.0.1",
  "private": true,
  "description": "Middleman between websockets and TCP sockets, basically acting as a TCP socket proxy for web apps.",
  "main": "lib/main",
  "types": "lib/main",
  "scripts": {
    "start": "npm run build:live",
    "build:live": "cd server & nodemon --exec ts-node main.ts --inspect"
  },
  "author": "Chris Croy",
  "dependencies": {
    "nodemon": "^1.11.0",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "@types/node": "^8.0.5",
    "@types/socket.io": "^1.4.31",
    "ts-node": "^3.1.0",
    "typescript": "^2.4.1"
  }
}
